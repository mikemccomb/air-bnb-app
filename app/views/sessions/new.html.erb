<h1>Login</h1>
<%= form_with method: "post", url: "/sessions" do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit "Submit" %>
  </div>
<% end%>

<p><b> If you have not make an account, click the link below to sign up </b></p>
  <div>
 <%= link_to "Signup", signup_path%>
 </div>